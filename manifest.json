{
  "manifest_version": 2,
  "name": "Zhihu Search",
  "version": "1.0",
  "description": "Quick way to search on zhihu while using chrome.",
  
  "icons":
  {
    "16": "images/16.png",
    "32": "images/32.png",
    "48": "images/48.png",
    "128": "images/64.png"
  },

  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "在知乎中搜索",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/64.png"
    }
  },

  "homepage_url": "https://github.com/Tokunaga-24253/zhihuSearch-on-Chrome"
  
  
}
